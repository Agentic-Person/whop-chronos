{
	"name": "chronos",
	"version": "0.1.0",
	"private": true,
	"description": "AI-powered video learning assistant for Whop creators in education and coaching",
	"scripts": {
		"dev": "whop-proxy --command=\"next dev --turbopack --port 3007\"",
		"build": "next build",
		"start": "next start",
		"lint": "biome lint",
		"type-check": "tsc --noEmit",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"verify-setup": "tsx scripts/verify-setup.ts",
		"validate": "npm run type-check && npm run lint",
		"db:migrate": "tsx scripts/run-migrations.ts",
		"db:seed": "tsx scripts/seed-database.ts",
		"seed:minimal": "dotenv -e .env.local -- tsx scripts/seed-minimal.ts",
		"seed:test": "dotenv -e .env.local -- tsx scripts/seed-test-environment.ts",
		"db:reset:test": "dotenv -e .env.local -- tsx scripts/reset-test-db.ts",
		"db:types": "supabase gen types typescript --linked > lib/db/types.ts",
		"db:reset": "supabase db reset",
		"db:push": "supabase db push",
		"test:video-pipeline": "tsx scripts/test-video-pipeline.ts",
		"test:analytics": "tsx scripts/test-analytics-dashboard.ts",
		"test:all": "tsx scripts/test-all-integrations.ts",
		"test:inngest": "tsx scripts/test-inngest.ts",
		"process-landing-transcript": "dotenv -e .env.local -- tsx scripts/process-landing-transcript.ts",
		"verify:storage": "dotenv -e .env.local -- tsx scripts/verify-storage.ts"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.68.0",
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@mux/mux-player-react": "^3.8.0",
		"@sentry/nextjs": "^10.23.0",
		"@supabase/ssr": "^0.7.0",
		"@supabase/supabase-js": "^2.80.0",
		"@tailwindcss/postcss": "^4.1.14",
		"@types/react-sparklines": "^1.7.5",
		"@upstash/ratelimit": "^2.0.7",
		"@vercel/functions": "^3.1.4",
		"@vercel/kv": "^3.0.0",
		"@whop/react": "0.3.0",
		"@whop/sdk": "0.0.3",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"framer-motion": "^12.23.24",
		"frosted-ui": "^0.0.1-canary.85",
		"inngest": "^3.45.0",
		"lucide-react": "^0.553.0",
		"next": "16.0.0",
		"openai": "^6.8.1",
		"react": "19.2.0",
		"react-countup": "^6.5.3",
		"react-dom": "19.2.0",
		"react-lite-youtube-embed": "^2.5.6",
		"react-sparklines": "^1.7.0",
		"react-youtube": "^10.1.0",
		"recharts": "^3.4.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.14",
		"youtubei.js": "^16.0.1",
		"yt-dlp-exec": "^1.0.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@playwright/test": "^1.56.1",
		"@types/node": "^20.19.21",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"@whop-apps/dev-proxy": "0.0.1-canary.117",
		"dotenv-cli": "^10.0.0",
		"pg": "^8.16.3",
		"tsx": "^4.19.2",
		"typescript": "^5.9.3"
	},
	"packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531",
	"pnpm": {
		"overrides": {
			"@types/react": "19.2.2",
			"@types/react-dom": "19.2.2"
		}
	}
}
